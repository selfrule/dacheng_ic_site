{
  "version": 1,
  "dfx": "0.11.1",
  "canisters": {
    "mysite_backend": {
      "type": "motoko",
      "main": "src/mysite_backend/main.mo"
    },
    "counter_backend": {
      "type": "motoko",
      "main": "src/counter_backend/main.mo"
    },
    "mysite_frontend": {
      "type": "assets",
      "source": [
        "src/mysite_frontend/assets"
      ],
      "dependencies": [
        "mysite_backend"
      ]
    },
    "counter_frontend": {
      "type": "assets",
      "source": [
        "src/counter_frontend/assets"
      ],
      "dependencies": [
        "counter_backend"
      ]
    }
  },
  "defaults": {
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}
